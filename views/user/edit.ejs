<body style="background-color:#fff;">
<div class="navbar navbar-default navbar-fixed-top" role="navigation">
<div class="container"> 
    
    <!--logo-->
   <a style="margin-top:-10px;" class="navbar-brand" href="index.html"><img  class="img-responsive" src="../images/user/logo2.png" alt="Eat Lyk Eagles"></a>
   
   
   
      <!-- Single button -->
<%- partial('../partials/user_logged_in') %>
</div>  
</div>






   <!--edit profile--> 
  <div class="container" style="margin-top:100px;">
    
      
      <!-- edit form column -->
      <div class="col-md-8 col-md-offset-2">

        <h3>Personal info</h3>
        <hr>
              <%- partial('../partials/error_display') %>
        <form action="updated" class="form-horizontal" role="form" method="post">
          <div class="form-group">
            <label class="col-lg-3 control-label">Name:</label>
            
            <div class="col-lg-8">
              <input name="name" class="form-control" type="text" value="<%= userobj.name %>">
            </div>
          </div>
          <div class="form-group">
            <label class="col-lg-3 control-label">Address:</label>
            <div class="col-lg-8">
              <input name="address" class="form-control" type="text" value="<%= userobj.address %>">
            </div>
          </div>
          <div class="form-group">
            <label class="col-lg-3 control-label">State::</label>
            <div class="col-lg-8">
              <input name="state" class="form-control" type="text" value="<%= userobj.state %>">
            </div>
          </div>
          <div class="form-group">
            <label class="col-lg-3 control-label">Country:</label>
            <div class="col-lg-8">
              <input name="country" class="form-control" type="text" value="<%= userobj.country %>">
            </div>
          </div>
          
          <div class="form-group">
            <label class="col-md-3 control-label">Pin:</label>
            <div class="col-md-8">
              <input name="pin" class="form-control" type="text" value="<%= userobj.pin %>">
            </div>
          </div>
            <div class="form-group">
            <label class="col-md-3 control-label">Phone:</label>
            <div class="col-md-8">
              <input name="phone" class="form-control" type="text" value="<%= userobj.phone %>">
            </div>
          </div>
          <div class="form-group">
            <label class="col-md-3 control-label">Old Password:</label>
            <div class="col-md-8">
              <input class="form-control" type="password" name="oldpassword" value="">
            </div>
          </div>
          <div class="form-group">
            <label class="col-md-3 control-label">New password:</label>
            <div class="col-md-8">
              <input class="form-control" type="password" name="password" value="">
            </div>
          </div>
          <div class="form-group">
            <label class="col-md-3 control-label"></label>
            <div class="col-md-8">
              <input type="submit" class="btn btn-primary" value="Save Changes">
              <span></span>
              <input type="reset" class="btn btn-default" value="Cancel">
            </div>
          </div>
        </form>
      </div>
  </div>
</div>
<hr><!--edit profil end-->  

<%- partial('../partials/footer') %>
                
            
</body>
</html>











